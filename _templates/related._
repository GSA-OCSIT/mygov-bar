<ul class="content-links"> 
  <% _.each(related, function(page) { %>
    <li id="related-<%= page.id %>"><a class="title" href="<%= page.url %>" target="_blank"><%= page.title %>
    <span class="domain"><%= page.domain.hostname.replace("www.","") %></span></a>
    </li>
  <% }) %>
</ul>

<div class="mini-feedback">

<div class="rating">
<h3>Was this page helpful?</h3>
<div id="stars" class="group"> </div>
</div>

</div>
